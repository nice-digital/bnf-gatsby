{
  "name": "@nice-digital/bnf-gatsby-functional-tests",
  "version": "1.0.0",
  "private": true,
  "description": "Browser-based BDD functional tests for BNF and BNFC",
  "scripts": {
    "test": "wdio run wdio.conf.ts",
    "test:docker": "npm test -- --hostname bnf-selenium-hub --baseUrl http://bnf-functional-tests.nice.org.uk",
    "wait:docker": "wait-on http://bnf-functional-tests.nice.org.uk --timeout 10000",
    "wait-then-test": "npm run wait:docker && npm run test:docker"
  },
  "keywords": [
    "wdio"
  ],
  "author": "",
  "license": "MIT",
  "volta": {
    "node": "14.18.2"
  },
  "devDependencies": {
    "@nice-digital/wdio-cucumber-steps": "^2.0.3",
    "@wdio/allure-reporter": "^7.17.3",
    "@wdio/cli": "^7.17.4",
    "@wdio/cucumber-framework": "^7.17.3",
    "@wdio/local-runner": "^7.17.4",
    "@wdio/selenium-standalone-service": "^7.17.3",
    "@wdio/spec-reporter": "^7.17.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "wait-on": "^6.0.1",
    "wdio-teamcity-reporter": "^3.0.2",
    "webdriverio": "^7.17.4"
  }
}
