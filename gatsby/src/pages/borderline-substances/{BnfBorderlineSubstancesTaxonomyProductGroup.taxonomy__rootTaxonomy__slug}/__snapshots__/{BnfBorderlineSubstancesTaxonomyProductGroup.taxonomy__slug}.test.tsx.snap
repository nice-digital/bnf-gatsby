"
	query ($id: String) {
		bnfBorderlineSubstancesTaxonomyProductGroup(id: { eq: $id }) {
			taxonomy {
				slug
				title
				rootTaxonomy {
					slug
					title
				}
				parentTaxonomy {
					slug
					title
					childTaxonomies {
						slug
						title
					}
				}
				substances {
					...FullSubstance
				}
				childTaxonomies {
					slug
					title
					substances {
						...FullSubstance
					}
					childTaxonomies {
						slug
						title
						substances {
							...FullSubstance
						}
						childTaxonomies {
							slug
							title
							substances {
								...FullSubstance
							}
						}
					}
				}
			}
		}
	}
"
`;
