{
  "name": "@nice-digital/bnf-gatsby",
  "version": "1.0.0",
  "private": true,
  "description": "Website for NICE British National Formulary (BNF) and British National Formulary for Children (BNFC). Built with Gatsby and using the NICE Design System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/bnf-gatsby.git"
  },
  "homepage": "https://github.com/nice-digital/bnf-gatsby#readme",
  "keywords": [
    "gatsby"
  ],
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/digital)",
    "Ian Routledge (http://ediblecode.com)"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:bnf": "cross-env GATSBY_SITE=bnf gatsby build",
    "postbuild:bnf": "cpy \"./**/*.*\" \"../../functional-tests/bnf-website-build\" --cwd=public --parents",
    "build:bnfc": "cross-env GATSBY_SITE=bnfc gatsby build",
    "postbuild:bnfc": "cpy \"./**/*.*\" \"../../functional-tests/bnfc-website-build\" --cwd=public --parents",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "// UTILS": "",
    "wait": "wait-on http-get://localhost:8000 --delay 10000 --interval 5000 --timeout 300000 --log",
    "audit:teamcity": "npm audit --audit-level=moderate --json|npm-audit-reporter-teamcity"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.15",
    "@fontsource/lora": "^4.5.13",
    "@nice-digital/browserslist-config": "^2.0.1",
    "@nice-digital/design-system": "^5.0.4",
    "@nice-digital/global-nav": "^5.0.928",
    "@nice-digital/search-client": "^2.0.1537",
    "gatsby": "4.6.1",
    "gatsby-plugin-catch-links": "^4.25.0",
    "gatsby-plugin-manifest": "^4.25.0",
    "gatsby-plugin-react-helmet": "^5.25.0",
    "gatsby-plugin-sass": "^5.25.0",
    "gatsby-plugin-sitemap": "^5.25.0",
    "gatsby-plugin-tsconfig-paths": "^1.0.6",
    "identity-obj-proxy": "^3.0.0",
    "lodash.throttle": "^4.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.62.1"
  },
  "volta": {
    "node": "14.18.2"
  },
  "devDependencies": {
    "@types/he": "^1.2.0",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^16.18.25",
    "@types/reach__router": "^1.3.11",
    "@types/react": "^17.0.58",
    "@types/react-dom": "^17.0.20",
    "@types/react-helmet": "^6.1.6",
    "axios": "^0.25.0",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "gatsby-plugin-google-tagmanager": "^4.25.0",
    "gatsby-plugin-offline": "^5.25.0",
    "gatsby-plugin-preact": "^6.25.0",
    "gatsby-plugin-remove-generator": "^1.3.0",
    "he": "^1.2.0",
    "http-proxy-middleware": "^2.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jszip": "^3.10.0",
    "npm-audit-reporter-teamcity": "^0.3.1",
    "preact": "^10.13.2",
    "preact-render-to-string": "^5.2.6",
    "process": "^0.11.10",
    "source-map-support": "^0.5.21",
    "striptags": "^3.2.0",
    "ts-node": "^10.9.1",
    "wait-on": "^6.0.0"
  },
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ]
}
